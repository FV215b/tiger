			     val yypos = i0+ !yygone
			open UserDeclarations Internal.StartStates
 in (yybufpos := i; case yyk of 

			(* Application actions *)

  1 => (newLine yypos; continue())
| 100 => (Tokens.NEQ (yypos, yypos + 2))
| 102 => (Tokens.EQ (yypos, yypos + 1))
| 104 => (Tokens.DIVIDE (yypos, yypos + 1))
| 106 => (Tokens.TIMES (yypos, yypos + 1))
| 108 => (Tokens.MINUS (yypos, yypos + 1))
| 110 => (Tokens.PLUS (yypos, yypos + 1))
| 112 => (Tokens.DOT (yypos, yypos + 1))
| 114 => (Tokens.LPAREN (yypos, yypos + 1))
| 116 => (Tokens.RPAREN (yypos, yypos + 1))
| 118 => (Tokens.LBRACK (yypos, yypos + 1))
| 12 => (Tokens.VAR  (yypos, yypos + 3))
| 120 => (Tokens.RBRACK (yypos, yypos + 1))
| 122 => (Tokens.LBRACE (yypos, yypos + 1))
| 124 => (Tokens.RBRACE (yypos, yypos + 1))
| 126 => (Tokens.SEMICOLON (yypos, yypos + 1))
| 128 => (Tokens.COLON (yypos, yypos + 1))
| 130 => (Tokens.COMMA (yypos, yypos + 1))
| 133 => let val yytext=yymktext() in Tokens.INT (valOf (Int.fromString yytext), yypos,yypos + size yytext) end
| 138 => let val yytext=yymktext() in Tokens.ID (yytext, yypos, yypos + size yytext) end
| 140 => (YYBEGIN STRING; currentString :=""; stringStartPos := yypos; continue())
| 142 => (YYBEGIN ESCAPE; continue())
| 144 => (YYBEGIN INITIAL; Tokens.STRING(!currentString, !stringStartPos, yypos + 1))
| 146 => let val yytext=yymktext() in ErrorMsg.error yypos ("illegal newline character " ^ yytext); continue() end
| 148 => let val yytext=yymktext() in appendS yytext; continue() end
| 150 => (newLine yypos; YYBEGIN DOUBLE_ESCAPE; continue())
| 152 => (YYBEGIN DOUBLE_ESCAPE; continue())
| 154 => (appendS "\n"; YYBEGIN STRING; continue())
| 156 => (appendS "\t"; YYBEGIN STRING; continue())
| 160 => let val yytext=yymktext() in dddToString (yytext, yypos); YYBEGIN STRING; continue() end
| 162 => (YYBEGIN CONTROL; continue())
| 164 => let val yytext=yymktext() in ErrorMsg.error yypos ("illegal escape character " ^ yytext); continue() end
| 166 => (YYBEGIN STRING; continue())
| 168 => (newLine yypos; continue())
| 170 => (continue())
